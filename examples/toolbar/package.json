{
  "name": "electric-toolbar",
  "version": "0.1.0",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "author": "ElectricSQL",
  "license": "Apache-2.0",
  "scripts": {
    "backend:down": "docker compose -f backend/compose/docker-compose.yaml down --volumes",
    "backend:start": "docker compose --env-file backend/compose/.envrc -f backend/compose/docker-compose.yaml up",
    "backend:stop": "yarn backend:down",
    "backend:up": "docker compose --env-file backend/compose/.envrc -f backend/compose/docker-compose.yaml up --detach",
    "client:generate": "npx electric-sql generate",
    "client:watch": "npx electric-sql generate --watch",
    "db:migrate": "node ./db/migrate.js",
    "db:psql": "node ./db/connect.js",
    "electric:start": "node ./backend/startElectric.js",
    "build": "node copy-wasm-files.js && node builder.js",
    "build-prod": "NODE_ENV=production npm run build",
    "start": "SERVE=true npm run build",
    "toolbar:build": "node toolbar-build.js && tsc --emitDeclarationOnly --outDir dist"
  },
  "engines": {
    "node": ">=16.11.0"
  },
  "dependencies": {
    "ace-builds": "^1.24.1",
    "react": "^18.2.0",
    "react-ace": "^10.1.0",
    "react-dom": "^18.2.0",
    "xterm-for-react": "^1.0.4"
  },
  "devDependencies": {
    "@databases/pg": "^5.4.1",
    "@electric-sql/prisma-generator": "latest",
    "@prisma/client": "4.8.1",
    "@types/node": ">=16.11.0",
    "@types/react": "^18.0.18",
    "@types/react-dom": "^18.0.11",
    "electric-sql": "latest",
    "esbuild": "^0.16.17",
    "esbuild-plugin-inline-image": "^0.0.9",
    "fs-extra": "^10.0.0",
    "prisma": "4.8.1",
    "shelljs": "^0.8.5",
    "typescript": "^4.4.3",
    "wa-sqlite": "rhashimoto/wa-sqlite#master"
  },
  "files": [
    "dist"
  ]
}
